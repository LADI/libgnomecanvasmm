namespace Gnome
{

CanvasRect::CanvasRect(CanvasGroup &parentx,
   double x1, double y1, double x2, double y2)
   : CanvasRE(GNOME_CANVAS_RE(g_object_new(get_type(),0)))
{
  initialize_class();
  item_construct(parentx);
  set("x1",x1,"y1",y1,"x2",x2,"y2",y2,0);
}

CanvasRect::CanvasRect(CanvasGroup &parentx)
   : CanvasRE(GNOME_CANVAS_RE(g_object_new(get_type(),0)))
{
  initialize_class();
  item_construct(parentx);
}

} /* namespace Gnome */
