# Build the generate_extra_defs utility executable.
# This is not used directly to build the rest of the project.

INCLUDES = $(LIBGNOMECANVASMM_CFLAGS)
LIBS = $(LIBGNOMECANVASMM_LIBS) -lgtkmm_generate_extra_defs-2.0

noinst_PROGRAMS = generate_extra_defs
generate_extra_defs_SOURCES = generate_defs_libgnomecanvas.cc
